{
  "name": "makeitlv/lavstore",
  "description": "Laravel e-commerce package for building online stores with ease.",
  "keywords": [
    "laravel",
    "lavstore",
    "store",
    "e-commerce",
    "cart",
    "shop"
  ],
  "license": "MIT",
  "homepage": "https://github.com/makeitlv/lavstore",
  "authors": [
    {
      "name": "Viktors Vipolzovs",
      "email": "viktors.vipolzovs@makeit.lv"
    }
  ],
  "require": {
    "php": "^8.3"
  },
  "require-dev": {
    "larastan/larastan": "^2.0",
    "laravel/pint": "^1.15",
    "nunomaduro/phpinsights": "^2.11",
    "orchestra/testbench": "^9.0",
    "pestphp/pest": "^2.34",
    "pestphp/pest-plugin-laravel": "^2.3"
  },
  "autoload": {
    "psr-4": {
      "Makeitlv\\Lavstore\\": "app/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Makeitlv\\Lavstore\\Tests\\": "tests/"
    }
  },
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "phpstan/extension-installer": true,
      "pestphp/pest-plugin": true,
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Makeitlv\\Lavstore\\Providers\\LavstoreServiceProvider"
      ]
    }
  }
}
